{
  "name": "heroku-cli-tinyadmin",
  "version": "1.0.0",
  "author": "Michael Bumann @bumi",
  "bugs": "https://github.com/bumi/heroku-cli-tinyadmin/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.3.0",
    "@heroku-cli/schema": "^1.0.25",
    "http-call": "^5.3.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.8",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.14",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/bumi/heroku-cli-tinyadmin",
  "keywords": [
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./src/commands",
    "bin": "heroku",
    "topics": {
      "tinyadmin": {
        "description": "creates a tinyadmin for the attached database"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bumi/heroku-cli-tinyadmin",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
